{
  "name": "mistfall",
  "version": "0.0.1",
  "description": "Type-safe schema builder and CRUD runtime for IndexedDB + in-memory adapters",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "indexeddb",
    "drizzle",
    "orm",
    "schema"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielphilipjohnson/mistfall.git"
  },
  "bugs": {
    "url": "https://github.com/danielphilipjohnson/mistfall/issues"
  },
  "homepage": "https://github.com/danielphilipjohnson/mistfall#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^22.8.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
